<h2>New User</h2>

<%= form_for @user, :url => create_user_path do |u| %>

  <p>
    <%= u.label :username %>
    <%= u.text_field :username %>
  </p>
  <p>
    <%= u.label :email %>
    <%= u.text_field :email %>
  </p>
  <p>  
    <%= u.label :password %>
    <%= u.password_field :password %>
  </p>
  <p>
    <%= u.label :password_confirmation %>
    <%= u.password_field :password_confirmation %>
  </p>
  <p>
    <%= u.submit %>
  </p>
  
    
<% end %>
  
<% form_for @email_user %>
  
  
 

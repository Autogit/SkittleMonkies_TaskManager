<h1>New User</h1>

<%= form_for @user, :url => users_path do |u| %>

  <%= u.label :email %>
  <%= u.text_field :email %>
  
  <%= u.label :password %>
  <%= u.text_field :password %>
  
  <%= u.label :password_confirmation %>
  <%= u.text_field :password_confirmation %>
  
  <%= u.submit %>
  
<% end %>  
<h2>New Task</h2>


<%= form_for @task do |f| %>

	<%= f.label :task_name %>
	<%= f.text_field :task_name %>

	<%= f.label :status %>
	<%= f.text_field :status %>

	<%= f.label :description %>
	<%= f.text_area :description %>
  
  <%= f.label :email %>
  <%= f.text_area :email %>

	<%= f.submit %>

<% end %>



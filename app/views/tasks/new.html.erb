<h2>New Task</h2>


<%= form_for @task do |f| %>

	<%= f.label :task_name %>
	<%= f.text_field :task_name %>

	<%= f.label :status %>
	<%= f.text_field :status %>

  	<%= f.label :email %>
  	<%= f.text_field :email %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.submit %>

<% end %>

